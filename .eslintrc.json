{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "root": true,
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn",
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
